<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:clickable="true"
    tools:context="com.orthopg.snaphy.orthopg.Fragment.ProfileFragment.ProfileFragment"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:id="@+id/fragment_other_profile_container"
    android:background="@color/white">

    <android.support.v7.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/white"
        android:id="@+id/fragment_other_profile_toolbar">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageButton
                android:id="@+id/fragment_other_profile_imageview1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/cross"
                android:background="@android:color/transparent"
                android:layout_alignParentLeft="true"
                android:padding="0dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_toRightOf="@+id/fragment_other_profile_imageview1"
                android:layout_marginLeft="20dp"
                android:textColor="@color/black"
                android:id="@+id/fragment_other_proile_textview1"
                android:text="Profile"/>


        </RelativeLayout>

    </android.support.v7.widget.Toolbar>

   <!-- <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >-->

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:id="@+id/fragment_other_profile_relative_layout1"
        android:layout_below="@+id/fragment_other_profile_toolbar"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:id="@+id/fragment_other_profile_linear_layout2"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="10dp">

            <de.hdodenhof.circleimageview.CircleImageView

                android:id="@+id/layout_profile_image"
                android:layout_width="100dp"
                android:layout_height="100dp"
                app:civ_border_width="0dp"
                app:civ_border_color="#FF000000"
                android:src="@mipmap/anonymous"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="15dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Email"
                    android:paddingLeft="10dp"
                    android:layout_marginTop="0dp"
                    android:textSize="9dp"
                    android:textStyle="bold"/>

                <TextView
                    android:id="@+id/fragment_other_profile_textview2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textColor="@color/buttonColor"
                    android:layout_marginBottom="5dp"
                    android:layout_marginLeft="10dp"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Registration Number"
                    android:paddingLeft="10dp"
                    android:layout_marginTop="5dp"
                    android:textSize="9dp"
                    android:textStyle="bold"/>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">


                    <TextView
                        android:id="@+id/fragment_other_profile_textview12"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textColor="@color/buttonColor"
                        android:layout_marginTop="2dp"
                        android:gravity="center"
                        android:layout_marginLeft="10dp"/>

                    <ImageButton
                        android:id="@+id/fragment_profile_imagebutton1"
                        android:layout_width="wrap_content"
                        android:layout_height="25dp"
                        android:background="@android:color/transparent"
                        android:layout_gravity="right"
                        android:layout_alignParentRight="true"
                        android:src="@mipmap/edit"/>

                </RelativeLayout>





                <!--<RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/fragment_profile_textview4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Orthopedic Trauma"
                        android:textColor="@color/buttonColor"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"/>

                    <ImageButton
                        android:id="@+id/fragment_profile_imagebutton2"
                        android:layout_width="wrap_content"
                        android:layout_height="25dp"
                        android:background="@android:color/transparent"
                        android:layout_gravity="right"
                        android:layout_alignParentRight="true"
                        android:src="@mipmap/edit"/>

                </RelativeLayout>-->

            </LinearLayout>

        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginRight="10dp"
            android:id="@+id/fragment_other_profile_relative_layout2"
            android:layout_below="@+id/fragment_other_profile_linear_layout2">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="name"
                android:id="@+id/fragment_other_profile_textview5"
                android:textColor="@color/buttonColor"
                android:layout_marginLeft="35dp"
                android:layout_marginBottom="10dp"
                android:layout_marginTop="10dp"/>


        </RelativeLayout>

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scrollbars="vertical"
            android:layout_below="@+id/fragment_other_profile_relative_layout2">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#EEE">

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:id="@+id/fragment_other_profile_cardview1"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="15dp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"


                            android:text="Speciality"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="5dp"
                            android:textStyle="bold"
                            android:id="@+id/fragment_other_profile_textview3"
                            android:textColor="@color/buttonColor"/>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginTop="10dp"
                            android:id="@+id/fragment_other_profile_relative_layout3"
                            android:layout_below="@+id/fragment_other_profile_textview3">


                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:id="@+id/fragment_other_profile_textview4"
                                android:layout_marginLeft="35dp"
                                android:layout_marginTop="5dp"/>

                            <ImageButton
                                android:id="@+id/fragment_profile_imagebutton2"
                                android:layout_width="wrap_content"
                                android:layout_height="25dp"
                                android:background="@android:color/transparent"
                                android:layout_gravity="right"
                                android:layout_marginBottom="5dp"
                                android:layout_alignParentRight="true"
                                android:src="@mipmap/edit"/>

                        </RelativeLayout>
                    </RelativeLayout>

                </android.support.v7.widget.CardView>


                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/fragment_other_profile_cardview2"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_below="@+id/fragment_other_profile_cardview1">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/fragment_other_profile_textview6"

                            android:text="Years Of Experience"
                            android:textStyle="bold"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="5dp"
                            android:textColor="@color/buttonColor"
                            />

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginTop="10dp"
                            android:id="@+id/fragment_other_profile_relative_layout4"
                            android:layout_below="@+id/fragment_other_profile_textview6">


                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="35dp"
                                android:id="@+id/fragment_other_profile_textview7"
                                android:layout_marginTop="5dp"
                                />


                            <ImageButton
                                android:id="@+id/fragment_profile_imagebutton3"
                                android:layout_width="wrap_content"
                                android:layout_height="25dp"
                                android:background="@android:color/transparent"
                                android:layout_gravity="right"
                                android:layout_alignParentRight="true"
                                android:layout_marginBottom="5dp"
                                android:src="@mipmap/edit"/>

                        </RelativeLayout>
                    </RelativeLayout>
                </android.support.v7.widget.CardView>

              <android.support.v7.widget.CardView
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content"
                  android:id="@+id/fragment_other_profile_cardview3"
                  android:layout_below="@+id/fragment_other_profile_cardview2"
                  android:layout_marginLeft="10dp"
                  android:layout_marginRight="10dp"
                  android:layout_marginTop="10dp">

                  <RelativeLayout
                      android:layout_width="match_parent"
                      android:layout_height="wrap_content">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/fragment_other_profile_textview8"
                            android:text="Current Working"
                            android:textStyle="bold"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="5dp"
                            android:textColor="@color/buttonColor"
                            />

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginTop="10dp"
                            android:id="@+id/fragment_other_profile_relative_layout5"
                            android:layout_below="@+id/fragment_other_profile_textview8">


                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="35dp"
                                android:id="@+id/fragment_other_profile_textview9"
                                android:layout_marginTop="5dp"
                                />


                            <ImageButton
                                android:id="@+id/fragment_profile_imagebutton4"
                                android:layout_width="wrap_content"
                                android:layout_height="25dp"
                                android:background="@android:color/transparent"
                                android:layout_gravity="right"
                                android:layout_alignParentRight="true"
                                android:layout_marginBottom="5dp"
                                android:src="@mipmap/edit"/>

                        </RelativeLayout>
                  </RelativeLayout>

              </android.support.v7.widget.CardView>

              <android.support.v7.widget.CardView
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content"
                  android:layout_marginTop="10dp"
                  android:layout_marginLeft="10dp"
                  android:layout_marginRight="10dp"
                  android:id="@+id/fragment_other_profile_cardview4"
                  android:layout_below="@+id/fragment_other_profile_cardview3">

                  <RelativeLayout
                      android:layout_width="match_parent"
                      android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/fragment_other_profile_textview10"
                        android:text="Qualification"
                        android:textStyle="bold"
                        android:layout_marginLeft="15dp"
                        android:layout_marginTop="5dp"
                        android:textColor="@color/buttonColor"
                        />


                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="10dp"
                        android:id="@+id/fragment_other_profile_relative_layout6"
                        android:layout_below="@+id/fragment_other_profile_textview10">


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="35dp"
                            android:id="@+id/fragment_other_profile_textview11"
                            android:layout_marginTop="5dp"
                            />


                        <ImageButton
                            android:id="@+id/fragment_profile_imagebutton5"
                            android:layout_width="wrap_content"
                            android:layout_height="25dp"
                            android:background="@android:color/transparent"
                            android:layout_gravity="right"
                            android:layout_alignParentRight="true"
                            android:layout_marginBottom="5dp"
                            android:src="@mipmap/edit"/>

                    </RelativeLayout>
                  </RelativeLayout>

              </android.support.v7.widget.CardView>


            </RelativeLayout>

        </ScrollView>
    </RelativeLayout>

    <!--</ScrollView>-->


    </RelativeLayout>
